{
  "name": "vite-vue3-h5-ssr",
  "type": "module",
  "version": "3.0.0",
  "packageManager": "pnpm@10.17.1",
  "author": "lincenying <lincenying@qq.com>",
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "serve": "nodemon --watch ./server.dev.ts --exec tsx ./server.dev.ts",
    "build": "npm run build:client && npm run build:server && npm run build:server:entry",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "cross-env VITE_APP_SSR=true vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:server:entry": "tsup --entry.server server.prod.ts --format esm --target es2020",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && tsx ./prerender.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix"
  },
  "dependencies": {
    "@lincy/utils": "^0.6.6",
    "@unhead/ssr": "^2.0.17",
    "@unhead/vue": "^2.0.17",
    "@vue/server-renderer": "^3.5.21",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.12.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "lodash": "^4.17.21",
    "lru-cache": "^11.2.2",
    "md5": "^2.3.0",
    "morgan": "^1.10.1",
    "pinia": "^3.0.3",
    "qs": "^6.14.0",
    "request-ip": "^3.3.0",
    "serve-static": "^2.2.0",
    "store2": "^2.14.4",
    "vant": "^4.9.21",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@lincy/eslint-config": "^5.8.1",
    "@lincy/stylelint-config": "^2.2.0",
    "@lincy/unocss-base-config": "^2.3.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.5.2",
    "@types/request-ip": "^0.0.41",
    "@unocss/eslint-plugin": "^66.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue-macros/volar": "^3.1.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "lint-staged": "^16.2.0",
    "mockjs": "^1.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "sass": "^1.93.2",
    "simple-git-hooks": "^2.13.1",
    "stylelint": "^16.24.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "unocss": "^66.5.2",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.7",
    "vite-plugin-inspect": "^11.3.3",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-progress": "^0.0.7",
    "vue-macros": "^3.1.0",
    "vue-tsc": "^3.0.8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "esbuild",
        "postcss",
        "prettier"
      ]
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "oxc-resolver",
      "simple-git-hooks"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
